

#include <iostream>

using namespace std;

int main()
{
    char ch;
    cin>>ch;
   
    if(ch=='A' || ch=='E'|| ch=='I'|| ch=='O'|| ch=='U'|| ch=='a'|| ch=='e'|| ch=='i' || ch=='o' || ch=='u')
    {
        cout<<"vowel"<<endl;
    }
      else if(!isalpha(ch))  //checking charaacter alphabhet or not
    {
        cout<<"not alphabhet"<<endl;
    }
    else
    {
        cout<<"consonat"<<endl;
    }

    return 0;
}
