
#include <iostream>
#include<string.h>
using namespace std;

int main()
{
    char ch[200];
    cin.get(ch,200);
    char str[200];
    int length=strlen(ch);
    int j=0;
    for(int i=0;i<length;i++)
    {
 if(ch[i]=='A'||ch[i]=='E'|| ch[i]=='I'|| ch[i]=='O'|| ch[i]=='U'|| ch[i]=='a'|| ch[i]=='e'|| ch[i]=='i' || ch[i]=='o' || ch[i]=='u')
        {
            continue;
        }
        else
        {
            str[j] = ch[i];
            j++;
        }
    }
    cout<<str;

    return 0;
}
