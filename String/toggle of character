#include<iostream>
#include<stdio.h>
using namespace std;
int main() {
    int i, count = 0;
    char str[200];
    gets(str);
    for(int i=0;str[i];i++)
    {
        if(str[i]>=65 && str[i]<=90)
        {
            char c=tolower(str[i]);
            cout<<c;
        }
        else
        {
            char d=toupper(str[i]);
            cout<<d;
            
        }
    }
    return 0;
}
