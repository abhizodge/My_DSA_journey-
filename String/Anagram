#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void anagram(string str1,string str2 )
{
    sort(str1.begin(),str1.end());
    sort(str2.begin(),str2.end());
    if(str1==str2)
    {
        cout<<"anagram";
    }
    else
    {
        cout<<"not a anagram";
    }
    
}
int main()
{
    string str1;
    string str2;
    cin>>str1;
    cin>>str2;
    anagram(str1,str2);

    return 0;
}
