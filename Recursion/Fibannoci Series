// User function Template for C++
class Solution {
  public:
    int nthFibonacci(int n){
        int a=0;
        int b=1;
        int c;
        for(int i=2;i<=n;i++)
        {
            c=(a%1000000007+b%1000000007)%1000000007; 
        
            a=b;
            b=c;
        }
        return c;
        
        
        // code here
    }
};
